                    "application/json"
                ],
                "tags": [
                    "deadlines"
                ],
                "summary": "List regulatory deadlines",
                "responses": {
                    "200": {
                        "description": "Example: {\\\"code\\\":\\\"A00\\\",\\\"message\\\":\\\"success\\\",\\\"data\\\":[]}",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            },
            "post": {
                "description": "Example payload:\n{\n\"title\": \"Quarterly BAS Lodgement\",\n\"category\": \"ATO_TAX\",\n\"dueDate\": \"2025-01-31\",\n\"notes\": \"Coordinate with accountant\"\n}",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "deadlines"
                ],
                "summary": "Create a new regulatory deadline",
